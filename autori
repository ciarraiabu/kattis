import java.util.*;
public class autori
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        String l=sc.nextLine();
        String del="-";
        String s[]=l.split(del);
        String v="";
        
        for(int i=0; i<s.length; i++)
        {
            v+=s[i].charAt(0);
        }
        System.out.print(v);

    }
}
